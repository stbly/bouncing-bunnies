bled to help with collision detection\nconst engineFrequence = 1000 / 120;\n\nconst runGame = () => {\n  setInterval(loop, engineFrequence);\n  setInterval(draw, drawFrequence);\n};\n\nconst loop = (delta) => {\n  Engine.update(engine, engineFrequence);\n  if (level) {\n    level.loop();\n  }\n};\n"},"lineCount":null}},"error":null,"hash":"533eda884088277b298c299d95949302","cacheData":{"env":{}}}